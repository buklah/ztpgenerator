system {
    time-zone America/Dallas;
    host-name {{ hostname }};
    domain-name consentfactory.com;
    auto-snapshot;
    authentication-order [ radius password ];
    ports {
        auxiliary port-type mini-usb;
    }
    root-authentication {
        encrypted-password "somethingsomethingsomethingsomething"; ## SECRET-DATA
    }
    root-authentication {
        encrypted-password "somethingsomethingsomethingsomething"; ## SECRET-DATA
    }
    name-server {
        192.168.100.200
    }
    services {
        netconf {
            ssh;
        }
        ssh {
            root-login deny;
        }
    }
    login {
        message "\n***\nThis is the property of Consentfactory.com. Kind of. Do not log-in without express permission!\n***\n ";
        class consentfactory-admins {
            idle-timeout 20;
            permissions all;
        }
        user consentfactoryadminofdoom {
            uid 2000;
            class consentfactory-admins;
            authentication {
                encrypted-password "somethingsomethingsomethingsomething"; ## SECRET-DATA
            }
        }
    }
    syslog {
        user * {
            any emergency;
        }
        host 192.168.100.150 {
            any error;
            change-log info;
            structured-data;
        }
    }
    
    ntp {
        server 192.168.100.200
    }
}
chassis {
    alarm {
        management-ethernet {
            link-down ignore;
        }
    }
}
