snmp {
    location "{{ snmp_location }}";
    community public {
        authorization read-only;
    }
}
routing-options {
    static {
        route 0.0.0.0/0 next-hop {{ gateway }};
    }
}
protocols {
    lldp {
        management-address {{ mgmt_ip }};
        port-id-subtype interface-name;
        interface all;
    }
    lldp-med {
        interface all;
    }
    igmp-snooping {
        vlan all {
            query-interval 60;
            immediate-leave;
        }
    }
    rstp {
        interface all;
        interface workstations201 {
            edge;
        }
        bpdu-block-on-edge;
    }
}
switch-options {
    voip {
        interface workstations201 {
            vlan PHONES;
            forwarding-class expedited-forwarding;
        }
    }
}
{{ vc_config }}
vlans {
    MGMT {
        vlan-id 101;
        l3-interface irb.101;
    }
    WORKSTATIONS {
        vlan-id 201;
    }
    PHONES{
        vlan-id 301;
    }
}
poe {
    interface all;
